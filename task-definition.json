{
    "family": "jinx-oc-task-def",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "jinx-oc-app",
            "image": "image",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8000,
                    "protocol": "tcp"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "0.5GB",
    "executionRoleArn": "arn:aws:iam::919924068817:role/ecsTaskExecutionRole"
}
